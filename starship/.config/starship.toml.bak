[battery]
full_symbol = "üîã"
charging_symbol = "üîå"
discharging_symbol = "‚ö°"

[[battery.display]]
threshold = 30
style = "bold red"

[character]
error_symbol = "[‚úñ](bold red) "
success_symbol = "[‚ùØ](bold green)" 

[cmd_duration]
format = ' \[[$duration]($style)\]'
min_time = 10

[directory]
truncation_length = 3
read_only = " Ô†Ω"
format = "on [$path]($style)[$lock_symbol]($lock_style) "

[git_branch]
format = "ÔÑÅ [$symbol$branch]($style) "
symbol = "ÓÇ† "
style = "bold yellow"

[git_commit]
commit_hash_length = 8
style = "bold white"

[git_state]
format = '[\($state( $progress_current of $progress_total)\)]($style) '

[git_status]
conflicted = "‚öîÔ∏è "
ahead = "‚á°√ó${count} "
behind = "‚á£√ó${count} "
diverged = "‚áï√ó${ahead_count} ÔÑá √ó${behind_count} "
untracked = "ÔÑ®√ó${count} "
stashed = " üì¶ "
modified = "ÔúÑ√ó${count} "
staged = "ÔÅß√ó${count} "
renamed = "ÔÑÅ√ó${count} "
deleted = "‚úò√ó${count} "
style = "bright-white"
format = "$all_status$ahead_behind"

[hostname]
ssh_only = false
format = "[$hostname]($style) "
trim_at = "-"
style = "bold #9e6ce0"
disabled = false

[memory_usage]
format = "$symbol[${ram}( | ${swap})]($style) "
threshold = 50 
style = "bold dimmed white"
disabled = false
symbol = " Ô°ö"

[package]
disabled = true
symbol = "Ô£ñ"

[username]
style_user = "bold green"
format = "[$user]($style) @ "
show_always = true

[time]
disabled = false

